<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WitdhRatioHeightLayout">
        <attr name="width_height_ratio" format="integer"></attr>
    </declare-styleable>
    <declare-styleable name="MaskImageOne">
        <attr name="mask_resource_id" format="reference"></attr>
        <!--string demension integer color boolean float  enum-->
       <!-- <declare-styleable name="名称">
            <attr name="orientation">
                <enum name="horizontal" value="0" />
                <enum name="vertical" value="1" />
            </attr>
        </declare-styleable>-->
        <attr name="src_resource_id" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="ZCViewPager">
        <attr name="style">
            <enum name="standard" value="0" />
            <enum name="tablet" value="1" />
            <enum name="cubein" value="2" />
            <enum name="cubeout" value="3" />
            <enum name="flipvertical" value="4" />
            <enum name="fliphorizontal" value="5" />
            <enum name="stack" value="6" />
            <enum name="zoomin" value="7" />
            <enum name="zoomout" value="8" />
            <enum name="rotateup" value="9" />
            <enum name="rotatedown" value="10" />
            <enum name="accordion" value="11" />
        </attr>
        <attr name="fadeEnabled" format="boolean" />
        <attr name="outlineEnabled" format="boolean" />
        <attr name="outlineColor" format="color|reference" />
    </declare-styleable>
    <declare-styleable name="ZCPayPasswordViewStyle">
        <attr name="bg_drawable" format="integer"/>
        <attr name="pwd_length" format="integer"/>
        <attr name="splide_line_width" format="float"/>
        <attr name="splide_line_color" format="color"/>
        <attr name="pwd_text_color" format="color"/>
        <attr name="pwd_text_size" format="dimension"/>
    </declare-styleable>
</resources>